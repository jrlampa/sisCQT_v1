{
  "name": "siscqt-enterprise-ai",
  "version": "1.0.0",
  "description": "Plataforma avançada de engenharia elétrica IM3 Brasil",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start": "node dist/server/server.js",
    "start:prod": "node dist/server/server.js",
    "start:prod:migrate": "npm run migrate:deploy && node dist/server/server.js",
    "start:dev": "node --import tsx server.ts",
    "dev": "concurrently \"vite\" \"prisma generate && tsx watch server.ts\"",
    "dev:client": "vite --host 0.0.0.0 --port 3000",
    "build:client": "vite build",
    "build:server": "prisma generate && tsc -p tsconfig.server.json",
    "build": "npm run build:client && npm run build:server",
    "gcp-build": "npm run build",
    "db:push": "npx prisma db push",
    "db:push:desktop": "prisma db push --schema prisma/schema.desktop.prisma",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:validate": "prisma validate",
    "prisma:validate:desktop": "prisma validate --schema prisma/schema.desktop.prisma",
    "prisma:generate:desktop": "prisma generate --schema prisma/schema.desktop.prisma",
    "test:unit": "vitest",
    "pretest:e2e": "playwright install",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test": "npm run test:unit && npm run test:e2e",
    "verify:xlsx-real": "tsx scripts/verifyRealXlsx.ts",
    "build:electron": "tsc -p tsconfig.electron.json",
    "electron:build": "npm run build && npm run build:electron",
    "electron:run": "electron dist/electron/main.js",
    "electron:start": "npm run electron:build && npm run electron:run",
    "dev:desktop": "concurrently -k \"npm run dev:client\" \"cross-env NODE_ENV=development tsx watch server.ts\" \"wait-on http://127.0.0.1:3000 && electron .\"",
    "build:desktop": "npm run prisma:generate:desktop && npm run build && npm run build:electron && electron-builder --win",
    "package:desktop": "npm run build:desktop --publish never",
    "release:desktop": "npm run build:desktop --publish always"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.11.0",
    "@azure/msal-react": "^2.0.12",
    "@google/genai": "^0.3.1",
    "@prisma/client": "^5.10.2",
    "@react-oauth/google": "^0.13.4",
    "compression": "^1.8.1",
    "cursor": "^0.1.5",
    "exceljs": "^4.4.0",
    "express": "^5.0.0",
    "express-rate-limit": "^8.2.1",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.1.0",
    "html2pdf.js": "^0.14.0",
    "jsonwebtoken": "^9.0.3",
    "jszip": "^3.10.1",
    "jwks-rsa": "^3.2.1",
    "leaflet": "^1.9.4",
    "multer": "^2.0.2",
    "prisma": "^5.10.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.12.2",
    "stripe": "^20.2.0",
    "tsx": "^4.7.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.58.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.11.24",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^37.10.3",
    "electron-builder": "^26.4.0",
    "electron-updater": "^6.7.3",
    "jsdom": "^27.4.0",
    "msal-react-tester": "^0.3.1",
    "postcss": "^8.5.6",
    "supertest": "^7.2.2",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vitest-mock-extended": "^3.1.0",
    "wait-on": "^8.0.5"
  }
}