{
  "name": "siscqt-enterprise-ai",
  "version": "1.0.0",
  "description": "Plataforma avançada de engenharia elétrica IM3 Brasil",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start": "node dist/server/server.js",
    "start:prod": "node dist/server/server.js",
    "start:prod:migrate": "npm run migrate:deploy && node dist/server/server.js",
    "start:dev": "node --import tsx server.ts",
    "dev": "concurrently \"vite\" \"prisma generate && tsx watch server.ts\"",
    "dev:client": "vite --host 0.0.0.0 --port 3000",
    "build:client": "vite build",
    "build:server": "prisma generate && tsc -p tsconfig.server.json",
    "build": "npm run build:client && npm run build:server",
    "db:push": "npx prisma db push",
    "migrate:deploy": "prisma migrate deploy",
    "test:unit": "vitest",
    "pretest:e2e": "playwright install",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test": "npm run test:unit && npm run test:e2e"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.11.0",
    "@azure/msal-react": "^2.0.12",
    "@google/genai": "^0.3.1",
    "@prisma/client": "^5.10.2",
    "compression": "^1.8.1",
    "cursor": "^0.1.5",
    "express": "^5.0.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "jszip": "^3.10.1",
    "jwks-rsa": "^3.2.1",
    "leaflet": "^1.9.4",
    "prisma": "^5.10.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.12.2",
    "tsx": "^4.7.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.58.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.24",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "jsdom": "^27.4.0",
    "msal-react-tester": "^0.3.1",
    "supertest": "^7.2.2",
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vitest-mock-extended": "^3.1.0"
  }
}
